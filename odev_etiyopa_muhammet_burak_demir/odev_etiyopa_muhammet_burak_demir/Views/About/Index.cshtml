@using odev_etiyopa_muhammet_burak_demir.Models.Siniflar
@model List<Hakkimizda>
@{ 
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_TestLayout.cshtml";
}

<div class="content">
    <div class="wrap">
        <div class="about">
            <h3>Etiyopya Hakkında</h3>
            @foreach (var x in Model)
            {
                <img src="@x.FotoUrl" alt="" />
                <p>@x.Aciklama</p>
            }
        </div>
    </div>
</div>


